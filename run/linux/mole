#!/bin/sh
#=========================================================================================
echo
echo zMole [Nutz.more], version1
echo
echo Setup class path ...
export JAVA_HOME=$JAVA_HOME
echo JAVA_HOME: $JAVA_HOME
export NUTZ_HOME=/home/zozoh/workspace/j2ee/nutz/bin
echo NUTZ_HOME: $NUTZ_HOME
