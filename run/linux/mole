echo MOLE_HOME: $MOLE_HOME
# Setup classpath
export CLASSPATH=.:$JAVA_HOME/lib/tools.jar:$JAVA_HOME/lib/dt.jar:$JAVA_HOME/jre/rt.jar
export CLASSPATH=$CLASSPATH:$MOLE_HOME:$NUTZ_HOME
